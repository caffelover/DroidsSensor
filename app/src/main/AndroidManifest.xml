<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caffelover.droidssensor" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Accelerater"
            android:label="@string/title_activity_accelerater" >
        </activity>
        <activity
            android:name=".MagneticField"
            android:label="@string/title_activity_magnetic_field" >
        </activity>
        <activity
            android:name=".Orientation"
            android:label="@string/title_activity_orientation" >
        </activity>
        <activity
            android:name=".Gyroscope"
            android:label="@string/title_activity_gyroscope" >
        </activity>
        <activity
            android:name=".Light"
            android:label="@string/title_activity_light" >
        </activity>
        <activity
            android:name=".Pressure"
            android:label="@string/title_activity_pressure" >
        </activity>
        <activity
            android:name=".Temperature"
            android:label="@string/title_activity_temperature" >
        </activity>
        <activity
            android:name=".Proximity"
            android:label="@string/title_activity_proximity" >
        </activity>
        <activity
            android:name=".Gravity"
            android:label="@string/title_activity_gravity" >
        </activity>
        <activity
            android:name=".LinearAcceleration"
            android:label="@string/title_activity_linear_acceleration" >
        </activity>
        <activity
            android:name=".Rotation_vector"
            android:label="@string/title_activity_rotation_vector" >
        </activity>
        <activity
            android:name=".Relative_humidity"
            android:label="@string/title_activity_relative_humidity" >
        </activity>
        <activity
            android:name=".Ambient_temperature"
            android:label="@string/title_activity_ambient_temperature" >
        </activity>
        <activity
            android:name=".Magnetic_field_uncalibrated"
            android:label="@string/title_activity_magnetic_field_uncalibrated" >
        </activity>
        <activity
            android:name=".Game_rotation_vector"
            android:label="@string/title_activity_game_rotation_vector" >
        </activity>
    </application>

</manifest>

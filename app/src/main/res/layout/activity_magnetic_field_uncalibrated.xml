<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:id="@+id/sensor_magnetic_field_uncalibrated"
    android:orientation="vertical"
    android:background="@color/black">
    <TextView
        style="@style/TextStyle1"
        android:id="@+id/magnetic_field_uncalibrated_name"/>
    <TextView
        style="@style/TextStyle2"
        android:id="@+id/x_uncalib"/>
    <TextView
        style="@style/TextStyle2"
        android:id="@+id/y_uncalib"/>
    <TextView
        style="@style/TextStyle2"
        android:id="@+id/z_uncalib"/>
    <TextView
        style="@style/TextStyle2"
        android:id="@+id/x_bias"/>
    <TextView
        style="@style/TextStyle2"
        android:id="@+id/y_bias"/>
    <TextView
        style="@style/TextStyle2"
        android:id="@+id/z_bias"/>
</LinearLayout>
